  "engineStrict": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 3001 --proxy-config proxy.conf.json",
    "build": "ng build",
    "production": "ng build --prod && npm run zip",
    "test": "ng test --watch=false",
    "e2e": "protractor -p protractor.conf.js --troubleshoot",
    "pree2e": "webdriver-manager update",
    "test:all": "npm run test && npm run e2e",
    "lint": "ng lint",
    "clean_install": "npm cache clean && rm -rf node_modules && npm install",
    "rebuild": "npm cache clean && rm -rf node_modules && npm install && npm run build",
    "zip": "zip dist/archive -r dist"
  },
  "lint-staged": {
    "*.{ts,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "git add"
    ]
  },
